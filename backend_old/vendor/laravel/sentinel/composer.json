{
    "name": "laravel/sentinel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        },
        {
            "name": "Mior Muhammad Zaki",
            "email": "mior@laravel.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "illuminate/container": "^8.37|^9.0|^10.0|^11.0|^12.0|^13.0"
    },
    "require-dev": {
        "laravel/pint": "^1.27",
        "orchestra/testbench": "^6.47.1|^7.56|^8.37|^9.16|^10.9|^11.0",
        "phpstan/phpstan": "^2.1.33"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\Sentinel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "App\\": "workbench/app/",
            "Database\\Factories\\": "workbench/database/factories/",
            "Database\\Seeders\\": "workbench/database/seeders/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Laravel\\Sentinel\\SentinelServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@clear",
            "@prepare",
            "@sync"
        ],
        "clear": "@php vendor/bin/testbench package:purge-skeleton --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": "@php vendor/bin/testbench workbench:build --ansi",
        "sync": [
            "@php vendor/bin/testbench workbench:sync --ansi",
            "@php vendor/bin/testbench vendor:publish --tag=laravel-assets --ansi"
        ],
        "serve": [
            "Composer\\Config::disableProcessTimeout",
            "@build",
            "@php vendor/bin/testbench serve --ansi"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
